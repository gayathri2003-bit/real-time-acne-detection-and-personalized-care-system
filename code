    <script>
        // Mobile menu toggle
        document.getElementById('mobile-menu-btn').addEventListener('click', function() {
            const mobileMenu = document.getElementById('mobile-menu');
            mobileMenu.classList.toggle('hidden');
        });

        // FAQ toggle
        document.querySelectorAll('.faq-item').forEach(item => {
            item.addEventListener('click', function() {
                const answer = this.querySelector('.faq-answer');
                const icon = this.querySelector('i');
                
                answer.classList.toggle('hidden');
                icon.classList.toggle('fa-chevron-down');
                icon.classList.toggle('fa-chevron-up');
            });
        });

        // Chatbot functionality
        const chatBubble = document.getElementById('chat-bubble');
        const chatWindow = document.getElementById('chat-window');
        const closeChat = document.getElementById('close-chat');
        const chatInput = document.getElementById('chat-input');
        const sendMessage = document.getElementById('send-message');
        const chatMessages = document.getElementById('chat-messages');

        chatBubble.addEventListener('click', function() {
            chatWindow.classList.toggle('hidden');
        });

        closeChat.addEventListener('click', function() {
            chatWindow.classList.add('hidden');
        });

        function addMessage(message, isUser = false) {
            const messageDiv = document.createElement('div');
            messageDiv.className = `mb-4 ${isUser ? 'text-right' : ''}`;
            
            const messageBubble = document.createElement('div');
            messageBubble.className = `inline-block max-w-xs p-3 rounded-lg text-sm ${
                isUser ? 'bg-purple-600 text-white' : 'bg-purple-100 text-gray-800'
            }`;
            messageBubble.textContent = message;
            
            messageDiv.appendChild(messageBubble);
            chatMessages.appendChild(messageDiv);
            chatMessages.scrollTop = chatMessages.scrollHeight;
        }

        function getBotResponse(userMessage) {
            const responses = {
                'hello': 'Hello! Welcome to MySkinny. How can I help you with your skincare today?',
                'hi': 'Hi there! I\'m here to help you with any questions about our AI skin analysis.',
                'how does it work': 'Our AI analyzes your facial image to detect acne, pores, pigmentation, and other skin conditions. Then we provide personalized skincare recommendations!',
                'is it accurate': 'Yes! Our AI uses advanced deep learning models like YOLO and TensorFlow for professional-grade accuracy.',
                'price': 'Our basic skin analysis is free! Premium features with detailed treatment plans are available with our subscription.',
                'default': 'That\'s a great question! Our AI can help analyze your skin for acne, pores, pigmentation, and more. Would you like to start your free analysis?'
            };
            
            const lowerMessage = userMessage.toLowerCase();
            for (let key in responses) {
                if (lowerMessage.includes(key)) {
                    return responses[key];
                }
            }
            return responses['default'];
        }

        sendMessage.addEventListener('click', function() {
            const message = chatInput.value.trim();
            if (message) {
                addMessage(message, true);
                chatInput.value = '';
                
                setTimeout(() => {
                    const botResponse = getBotResponse(message);
                    addMessage(botResponse);
                }, 1000);
            }
        });

        chatInput.addEventListener('keypress', function(e) {
            if (e.key === 'Enter') {
                sendMessage.click();
            }
        });

        // Analysis modal functionality
        const analysisModal = document.getElementById('analysis-modal');
        const resultsModal = document.getElementById('results-modal');
        const webcamModal = document.getElementById('webcam-modal');
        const assessmentModal = document.getElementById('assessment-modal');
        const closeModal = document.getElementById('close-modal');
        const closeResults = document.getElementById('close-results');
        const closeWebcam = document.getElementById('close-webcam');
        const closeAssessment = document.getElementById('close-assessment');

        let userAssessment = {};

        function startAnalysis() {
            analysisModal.classList.remove('hidden');
        }

        function closeAllModals() {
            analysisModal.classList.add('hidden');
            resultsModal.classList.add('hidden');
            webcamModal.classList.add('hidden');
            assessmentModal.classList.add('hidden');
        }

        // Close modal event listeners
        closeModal.addEventListener('click', closeAllModals);
        closeResults.addEventListener('click', closeAllModals);
        closeWebcam.addEventListener('click', closeAllModals);
        closeAssessment.addEventListener('click', closeAllModals);

        // Image upload functionality
        document.getElementById('image-upload').addEventListener('change', function(e) {
            const file = e.target.files[0];
            if (file) {
                processImage(file);
            }
        });

        // Webcam functionality
        function startWebcamAnalysis() {
            webcamModal.classList.remove('hidden');
            analysisModal.classList.add('hidden');
            
            navigator.mediaDevices.getUserMedia({ video: true })
                .then(stream => {
                    const video = document.getElementById('webcam-video');
                    video.srcObject = stream;
                })
                .catch(err => {
                    alert('Unable to access webcam. Please check permissions.');
                    console.error('Webcam error:', err);
                });
        }

        document.getElementById('capture-photo').addEventListener('click', function() {
            const video = document.getElementById('webcam-video');
            const canvas = document.getElementById('webcam-canvas');
            const ctx = canvas.getContext('2d');
            
            canvas.width = video.videoWidth;
            canvas.height = video.videoHeight;
            ctx.drawImage(video, 0, 0);
            
            canvas.toBlob(blob => {
                processImage(blob);
            });
            
            // Stop webcam
            const stream = video.srcObject;
            if (stream) {
                stream.getTracks().forEach(track => track.stop());
            }
            webcamModal.classList.add('hidden');
        });

        // Assessment functionality
        function startAssessment() {
            assessmentModal.classList.remove('hidden');
            analysisModal.classList.add('hidden');
        }

        document.getElementById('assessment-form').addEventListener('submit', function(e) {
            e.preventDefault();
            const formData = new FormData(e.target);
            userAssessment = {
                concern: formData.get('concern'),
                skinType: formData.get('skinType'),
                breakouts: formData.get('breakouts')
            };
            assessmentModal.classList.add('hidden');
            analysisModal.classList.remove('hidden');
        });

        // Image processing and analysis
        function processImage(file) {
            analysisModal.classList.add('hidden');
            
            // Show processing screen
            document.getElementById('analysis-start').classList.add('hidden');
            document.getElementById('analysis-processing').classList.remove('hidden');
            analysisModal.classList.remove('hidden');
            
            // Simulate AI processing with progress bar
            let progress = 0;
            const progressBar = document.getElementById('progress-bar');
            const progressText = document.getElementById('progress-text');
            
            const progressSteps = [
                'Initializing analysis...',
                'Detecting facial features...',
                'Analyzing skin texture...',
                'Identifying acne and blemishes...',
                'Measuring pore visibility...',
                'Detecting pigmentation...',
                'Assessing hydration levels...',
                'Generating recommendations...',
                'Finalizing results...'
            ];
            
            const progressInterval = setInterval(() => {
                progress += Math.random() * 15 + 5;
                if (progress > 100) progress = 100;
                
                progressBar.style.width = progress + '%';
                const stepIndex = Math.floor((progress / 100) * progressSteps.length);
                if (stepIndex < progressSteps.length) {
                    progressText.textContent = progressSteps[stepIndex];
                }
                
                if (progress >= 100) {
                    clearInterval(progressInterval);
                    setTimeout(() => {
                        showResults(file);
                    }, 500);
                }
            }, 200);
        }

        function showResults(imageFile) {
            analysisModal.classList.add('hidden');
            
            // Display uploaded image
            const reader = new FileReader();
            reader.onload = function(e) {
                const uploadedImage = document.getElementById('uploaded-image');
                uploadedImage.innerHTML = `<img src="${e.target.result}" class="w-full h-full object-cover rounded-lg" alt="Analyzed image">`;
            };
            reader.readAsDataURL(imageFile);
            
            // Generate AI analysis results
            generateAnalysisResults();
            
            resultsModal.classList.remove('hidden');
        }

        function generateAnalysisResults() {
            // Simulate AI detection results
            const detectionResults = [
                { condition: 'Acne', severity: 'Moderate', confidence: '92%', color: 'text-red-600' },
                { condition: 'Enlarged Pores', severity: 'Mild', confidence: '87%', color: 'text-orange-600' },
                { condition: 'Uneven Skin Tone', severity: 'Mild', confidence: '78%', color: 'text-yellow-600' },
                { condition: 'Dehydration', severity: 'Moderate', confidence: '85%', color: 'text-blue-600' }
            ];
            
            const detectionContainer = document.getElementById('detection-results');
            detectionContainer.innerHTML = detectionResults.map(result => `
                <div class="flex justify-between items-center p-3 bg-gray-50 rounded-lg">
                    <div>
                        <span class="font-medium ${result.color}">${result.condition}</span>
                        <span class="text-sm text-gray-600 ml-2">(${result.severity})</span>
                    </div>
                    <span class="text-sm font-medium text-gray-700">${result.confidence}</span>
                </div>
            `).join('');
            
            // Generate personalized recommendations based on assessment
            const concern = userAssessment.concern || 'acne';
            const skinType = userAssessment.skinType || 'combination';
            
            generateRecommendations(concern, skinType);
        }

        function generateRecommendations(concern, skinType) {
            const recommendations = {
                acne: {
                    selfGuidance: [
                        "Wash your face twice daily with a gentle, non-comedogenic cleanser",
                        "Avoid touching or picking at acne lesions to prevent scarring",
                        "Use oil-free, non-comedogenic moisturizers and makeup",
                        "Change pillowcases regularly and keep hair clean",
                        "Maintain a consistent skincare routine for best results"
                    ],
                    treatment: [
                        "Salicylic acid (BHA) 0.5-2% for gentle exfoliation",
                        "Benzoyl peroxide 2.5-5% for antibacterial action",
                        "Niacinamide 5-10% to reduce inflammation",
                        "Retinoids (start with retinol 0.25%) for cell turnover",
                        "Consider professional treatments like chemical peels"
                    ],
                    causes: [
                        "Hormonal fluctuations, especially during puberty or menstrual cycles",
                        "Excess sebum production clogging pores",
                        "Bacterial overgrowth (P. acnes) in blocked pores",
                        "Genetic predisposition to acne",
                        "Stress, diet, and certain cosmetic products"
                    ],
                    routine: {
                        morning: [
                            "Gentle cleanser with salicylic acid",
                            "Niacinamide serum",
                            "Oil-free moisturizer",
                            "Broad-spectrum SPF 30+ sunscreen"
                        ],
                        evening: [
                            "Double cleanse (oil cleanser + gentle cleanser)",
                            "Benzoyl peroxide spot treatment",
                            "Retinol serum (2-3x per week)",
                            "Lightweight night moisturizer"
                        ]
                    }
                },
                pigmentation: {
                    selfGuidance: [
                        "Always wear broad-spectrum SPF 30+ sunscreen daily",
                        "Avoid prolonged sun exposure, especially during peak hours",
                        "Use gentle exfoliation to promote cell turnover",
                        "Be patient - pigmentation takes 3-6 months to improve",
                        "Avoid harsh scrubbing which can worsen pigmentation"
                    ],
                    treatment: [
                        "Vitamin C serum 10-20% for antioxidant protection",
                        "Hydroquinone 2% for targeted lightening",
                        "Alpha hydroxy acids (AHA) for gentle exfoliation",
                        "Kojic acid or arbutin for natural lightening",
                        "Professional treatments like IPL or laser therapy"
                    ],
                    causes: [
                        "UV damage from sun exposure over time",
                        "Post-inflammatory hyperpigmentation from acne",
                        "Hormonal changes (melasma) during pregnancy",
                        "Natural aging process and decreased cell turnover",
                        "Certain medications or medical conditions"
                    ],
                    routine: {
                        morning: [
                            "Gentle vitamin C cleanser",
                            "Vitamin C serum 15%",
                            "Hydrating moisturizer",
                            "Broad-spectrum SPF 50+ sunscreen"
                        ],
                        evening: [
                            "Gentle cleanser",
                            "AHA exfoliant (2-3x per week)",
                            "Hydroquinone treatment",
                            "Rich night moisturizer with ceramides"
                        ]
                    }
                }
            };
            
            const currentRec = recommendations[concern] || recommendations.acne;
            
            // Populate self guidance
            document.getElementById('self-guidance').innerHTML = 
                '<ul class="space-y-2">' + 
                currentRec.selfGuidance.map(item => `<li>• ${item}</li>`).join('') + 
                '</ul>';
            
            // Populate treatment options
            document.getElementById('treatment-options').innerHTML = 
                '<ul class="space-y-2">' + 
                currentRec.treatment.map(item => `<li>• ${item}</li>`).join('') + 
                '</ul>';
            
            // Populate causes
            document.getElementById('causes').innerHTML = 
                '<ul class="space-y-2">' + 
                currentRec.causes.map(item => `<li>• ${item}</li>`).join('') + 
                '</ul>';
            
            // Populate daily routine
            document.getElementById('daily-routine').innerHTML = `
                <div class="space-y-4">
                    <div>
                        <h5 class="font-medium mb-2">🌅 Morning Routine:</h5>
                        <ol class="space-y-1 text-sm">
                            ${currentRec.routine.morning.map((step, i) => `<li>${i+1}. ${step}</li>`).join('')}
                        </ol>
                    </div>
                    <div>
                        <h5 class="font-medium mb-2">🌙 Evening Routine:</h5>
                        <ol class="space-y-1 text-sm">
                            ${currentRec.routine.evening.map((step, i) => `<li>${i+1}. ${step}</li>`).join('')}
                        </ol>
                    </div>
                </div>
            `;
        }

        function downloadReport() {
            // Get current analysis results
            const detectionResults = document.getElementById('detection-results').textContent;
            const selfGuidance = document.getElementById('self-guidance').textContent;
            const treatmentOptions = document.getElementById('treatment-options').textContent;
            const causes = document.getElementById('causes').textContent;
            const dailyRoutine = document.getElementById('daily-routine').textContent;
            
            // Create comprehensive report content
            const reportContent = `MySkinny Skin Analysis Report
============================

Analysis Date: ${new Date().toLocaleDateString()}
Analysis Time: ${new Date().toLocaleTimeString()}

DETECTED CONDITIONS:
${detectionResults || '- Acne (Moderate severity, 92% confidence)\n- Enlarged Pores (Mild severity, 87% confidence)\n- Uneven Skin Tone (Mild severity, 78% confidence)\n- Dehydration (Moderate severity, 85% confidence)'}

SELF GUIDANCE:
${selfGuidance || 'Please refer to your personalized guidance in the detailed analysis.'}

TREATMENT OPTIONS:
${treatmentOptions || 'Please refer to your personalized treatment options in the detailed analysis.'}

POSSIBLE CAUSES:
${causes || 'Please refer to the causes section in your detailed analysis.'}

DAILY ROUTINE:
${dailyRoutine || 'Please refer to your personalized routine in the detailed analysis.'}

IMPORTANT NOTES:
- This analysis is for informational purposes only
- Consult a dermatologist for persistent or severe skin concerns
- Results may vary based on individual skin conditions
- Maintain consistency with recommended routines for best results

Generated by MySkinny AI Analysis System
Visit: https://myskinny.com for more information

============================
Report End`;
            
            try {
                // Create and download the file
                const blob = new Blob([reportContent], { type: 'text/plain;charset=utf-8' });
                const url = URL.createObjectURL(blob);
                const a = document.createElement('a');
                a.style.display = 'none';
                a.href = url;
                a.download = `MySkinny_Analysis_Report_${new Date().toISOString().split('T')[0]}.txt`;
                
                // Add to DOM, click, and remove
                document.body.appendChild(a);
                a.click();
                document.body.removeChild(a);
                
                // Clean up the URL object
                setTimeout(() => URL.revokeObjectURL(url), 100);
                
                // Show success message
                showNotification('Report downloaded successfully!', 'success');
                
            } catch (error) {
                console.error('Download error:', error);
                showNotification('Download failed. Please try again.', 'error');
            }
        }

        function showNotification(message, type = 'info') {
            // Create notification element
            const notification = document.createElement('div');
            notification.className = `fixed top-4 right-4 z-50 p-4 rounded-lg shadow-lg transition-all duration-300 ${
                type === 'success' ? 'bg-green-500 text-white' : 
                type === 'error' ? 'bg-red-500 text-white' : 
                'bg-blue-500 text-white'
            }`;
            notification.innerHTML = `
                <div class="flex items-center">
                    <i class="fas ${type === 'success' ? 'fa-check-circle' : type === 'error' ? 'fa-exclamation-circle' : 'fa-info-circle'} mr-2"></i>
                    <span>${message}</span>
                </div>
            `;
            
            // Add to page
            document.body.appendChild(notification);
            
            // Remove after 3 seconds
            setTimeout(() => {
                notification.style.opacity = '0';
                notification.style.transform = 'translateX(100%)';
                setTimeout(() => {
                    if (notification.parentNode) {
                        document.body.removeChild(notification);
                    }
                }, 300);
            }, 3000);
        }

        function shareResults() {
            if (navigator.share) {
                navigator.share({
                    title: 'My MySkinny Analysis Results',
                    text: 'I just got my personalized skin analysis from MySkinny AI! Check out this amazing skincare platform.',
                    url: window.location.href
                });
            } else {
                // Fallback for browsers that don't support Web Share API
                const shareText = 'I just got my personalized skin analysis from MySkinny AI! Check it out: ' + window.location.href;
                navigator.clipboard.writeText(shareText).then(() => {
                    alert('Share link copied to clipboard!');
                });
            }
        }

        // Close modals when clicking outside
        [analysisModal, resultsModal, webcamModal, assessmentModal].forEach(modal => {
            modal.addEventListener('click', function(e) {
                if (e.target === modal) {
                    closeAllModals();
                }
            });
        });

        // Smooth scrolling for navigation links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Add fade-in animation on scroll
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver(function(entries) {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('fade-in');
                }
            });
        }, observerOptions);

        document.querySelectorAll('section').forEach(section => {
            observer.observe(section);
        });
    </script>
